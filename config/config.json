{
  "theme": {
    "slug": "eflight",
    "name": "eFlight",
    "author": "Robert Nobles",
    "PHPNamespace": "WP_Rig\\WP_Rig"
  },
  "dev": {
    "browserSync": {
      "live": true,
      "proxyURL": "wprig.test:8888",
      "bypassPort": "8181",
      "https": true
    },
    "styles": {
      "stage": 3,
      "autoprefixer": {
        "grid": false
      },
      "importFrom": ["_custom-media.css"],
      "features": {
        "custom-media-queries": true,
        "custom-properties": true,
        "nesting-rules": true
      }
    },
    "debug": {
      "styles": false,
      "scripts": false,
      "phpcs": true
    }
  },
  "export": {
    "compress": true,
    "generatePotFile": true,
    "filesToCopy": ["LICENSE", "readme.txt", "screenshot.png"]
  }
}
